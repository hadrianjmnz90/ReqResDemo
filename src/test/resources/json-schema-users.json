{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["id", "name", "username", "email", "address"],
    "properties": {
      "id": { "type": "integer" },
      "name": { "type": "string" },
      "username": { "type": "string" },
      "email": { "type": "string", "format": "email" },
      "address": {
        "type": "object",
        "required": ["street", "city", "zipcode"],
        "properties": {
          "street": { "type": "string" },
          "suite": { "type": "string" },
          "city": { "type": "string" },
          "zipcode": { "type": "string" }
        }
      },
      "phone": { "type": "string" },
      "website": { "type": "string" },
      "company": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "catchPhrase": { "type": "string" },
          "bs": { "type": "string" }
        }
      }
    }
  }
}